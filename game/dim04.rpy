label dim04:
    stop music fadeout 2.0
    scene bg blackbackground
    with dissolve_scene_full
    $ s_name = "С. Найт"
    $ n_name = "С-т Нацуки"
    $ y_name = "Проф. Юри"
    $ m_name = "Д-р Баг"
    $ pm_name = "Моника Прайм"
    $ pmc_name = "[player] Прайм"
    $ ps_name = "Сайори Прайм"
    $ pn_name = "Нацуки Прайм"
    $ py_name = "Юри Прайм"
